var recommendedItems=[]

jQuery(".recommended-items .repeat-wrapper").each(function( index ) {
	var foodImgSrc = $(this).find("img").attr("src");
	var foodTitle = $(this).find("h5.recommended.text-ellipsis").text();
	var foodPrice = $(this).find(".item-price").text();
	console.log( index+","+
	foodTitle.trim() +","+
	foodPrice +","+
	foodImgSrc );
});
